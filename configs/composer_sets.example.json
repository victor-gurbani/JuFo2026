        "Bach": [
          "\\bbach\\b",
          "\\bj\\.?\\s*s\\.?\\s*bach\\b",
          "\\bjs\\s*bach\\b",
          "\\bjohann\\s+sebastian\\s+bach\\b",
          "\\bbach,?\\s*j\\.?\\s*s\\.?\\b",
          "\\bbach,?\\s*johann\\s+sebastian\\b"
        ],
        "Mozart": [
          "\\bmozart\\b",
          "\\bw\\.?\\s*a\\.?\\s*mozart\\b",
          "\\bwolfgang\\s+amadeus\\s+mozart\\b",
          "\\bmozart,?\\s*wolfgang\\b"
        ],
        "Chopin": [
          "\\bchopin\\b",
          "\\bfr[eé]d[eé]ric\\s+chopin\\b",
          "\\bfrederic\\s+chopin\\b",
          "\\bchopin,?\\s*frederic\\b"
